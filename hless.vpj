		</Folder>
		<Folder
			Name="Header Files"
			Filters="*.h;*.H;*.hh;*.hpp;*.hxx;*.inc;*.sh;*.cpy;*.if">
			<F N="/works/hless/src/brush.h"/>
			<F N="/works/hless/src/buffer.h"/>
