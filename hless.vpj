			<F N="/works/hless/src/readable.h"/>
			<F N="/works/hless/src/screen.h"/>
			<F N="/works/hless/src/types.h"/>
		</Folder>
		<Folder
			Name="Resource Files"
			Filters="*.ico;*.cur;*.dlg"/>
		<Folder
			Name="Bitmaps"
			Filters="*.bmp"/>
		<Folder
			Name="Other Files"
			Filters="">
			<F N="/works/hless/py_hless/old/hless-0.2"/>
			<F N="/works/hless/py_hless/hless.txt"/>
			<F
				N="Makefile"
				Type="Makefile"/>
		</Folder>
	</Files>
</Project>
